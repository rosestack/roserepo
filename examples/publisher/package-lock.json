{
  "name": "publisher",
  "lockfileVersion": 3,
  "requires": true,
  "packages": {
    "": {
      "name": "publisher",
      "workspaces": [
        "packages/*"
      ],
      "dependencies": {
        "roserepo": "file:../../../packages/roserepo"
      }
    },
    "../../../packages/roserepo": {
      "version": "1.0.11",
      "license": "mit",
      "dependencies": {
        "@meslzy/config": "^1.0.7",
        "chalk": "^5.3.0",
        "chokidar": "^3.5.3",
        "commander": "^11.1.0",
        "dotenv": "16.3.1",
        "execa": "^8.0.1",
        "fast-glob": "^3.3.2",
        "micromatch": "^4.0.5"
      },
      "bin": {
        "roserepo": "dist/bin.js"
      },
      "devDependencies": {
        "@types/micromatch": "^4.0.6"
      },
      "peerDependencies": {
        "typescript": ">=5"
      },
      "peerDependenciesMeta": {
        "typescript": {
          "optional": true
        }
      }
    },
    "../../packages/roserepo": {
      "version": "1.0.11",
      "extraneous": true,
      "license": "mit",
      "dependencies": {
        "@meslzy/config": "^1.0.7",
        "chalk": "^5.3.0",
        "chokidar": "^3.5.3",
        "commander": "^11.1.0",
        "dotenv": "16.3.1",
        "execa": "^8.0.1",
        "fast-glob": "^3.3.2"
      },
      "bin": {
        "roserepo": "dist/bin.js"
      },
      "devDependencies": {
        "@types/micromatch": "^4.0.6"
      },
      "peerDependencies": {
        "typescript": ">=5"
      },
      "peerDependenciesMeta": {
        "typescript": {
          "optional": true
        }
      }
    },
    "apps/backend/app": {
      "name": "@backend/app",
      "extraneous": true,
      "dependencies": {
        "@backend/pkg1": "*",
        "@backend/pkg2": "*"
      }
    },
    "apps/backend/packages/pkg1": {
      "name": "@backend/pkg1",
      "extraneous": true,
      "dependencies": {
        "pkg1": "*"
      }
    },
    "apps/backend/packages/pkg2": {
      "name": "@backend/pkg2",
      "extraneous": true,
      "dependencies": {
        "pkg2": "*"
      }
    },
    "apps/frontend/app": {
      "name": "@frontend/app",
      "extraneous": true,
      "dependencies": {
        "@frontend/pkg1": "*",
        "@frontend/pkg2": "*"
      }
    },
    "apps/frontend/packages/pkg1": {
      "name": "@frontend/pkg1",
      "extraneous": true,
      "dependencies": {
        "pkg1": "*"
      }
    },
    "apps/frontend/packages/pkg2": {
      "name": "@frontend/pkg2",
      "extraneous": true,
      "dependencies": {
        "pkg2": "*"
      }
    },
    "node_modules/pkg1": {
      "resolved": "packages/pkg1",
      "link": true
    },
    "node_modules/pkg2": {
      "resolved": "packages/pkg2",
      "link": true
    },
    "node_modules/pkg3": {
      "resolved": "packages/pkg3",
      "link": true
    },
    "node_modules/roserepo": {
      "resolved": "../../../packages/roserepo",
      "link": true
    },
    "packages/pkg1": {
      "version": "1.0.0"
    },
    "packages/pkg2": {
      "version": "1.0.0"
    },
    "packages/pkg3": {
      "version": "1.0.0"
    }
  }
}
